<xaml:BaseMainWindow x:Class="Com.Ericmas001.Windows.Demo.TabControlApp.MainWindow"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:xaml="clr-namespace:Com.Ericmas001.Windows.Xaml;assembly=Com.Ericmas001.Windows.Xaml"
                    xmlns:viewModels="clr-namespace:Com.Ericmas001.Windows.Demo.TabControlApp.ViewModels"
                    Icon="/Com.Ericmas001.Windows.Demo.TabControlApp;component/Resources/demo.png">

    <Window.DataContext>
        <viewModels:MainViewModel />
    </Window.DataContext>
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Templates/NewTabTemplates.xaml"/>
                <ResourceDictionary Source="/Templates/ResultTemplates.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
</xaml:BaseMainWindow>